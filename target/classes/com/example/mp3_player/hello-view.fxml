            <Font name="Bodoni MT Black" size="14.0" />
         </font>
      </Button>
      <Button fx:id="resetMedia" layoutX="156.0" layoutY="156.0" mnemonicParsing="false" onAction="#resetMedia" prefHeight="63.0" prefWidth="74.0" text="RESET">
