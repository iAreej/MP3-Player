<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="pane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="220.0" prefWidth="684.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mp3_player.HelloController">
   <children>
      <ImageView fx:id="myimage" fitHeight="235.0" fitWidth="696.0" layoutX="-12.0" layoutY="-9.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images.jpg" />
         </image>
      </ImageView>

      <Label fx:id="songLabel" alignment="CENTER" layoutX="-2.0" layoutY="2.0" prefHeight="126.0" prefWidth="682.0" text="MP3 PLAYER" textFill="#150707">
