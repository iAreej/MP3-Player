         <font>
            <Font name="Bodoni MT Black" size="18.0" />
         </font>
      </Button>
      <Button fx:id="pauseMedia" layoutX="82.0" layoutY="156.0" mnemonicParsing="false" onAction="#pauseMedia" prefHeight="63.0" prefWidth="74.0" text="PAUSE">
